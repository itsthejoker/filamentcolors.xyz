{% load static %}

<swatch-card
  id="{{ obj.id }}"
  showColormatchExtras="{{ show_colormatch_extras }}"
  color="{{ obj.hex_color }}"
  mfr="{{ obj.manufacturer.name }}"
  name="{{ obj.color_name }}"
  type="{{ obj.filament_type.name }}"
  slug="{{ obj.slug }}"
  td="{{ obj.calculated_td }}"
  available="{{ obj.is_available }}"
  cardImgUrl="{{ obj.card_img.url }}"
  distance="{{ obj.distance }}"
></swatch-card>
