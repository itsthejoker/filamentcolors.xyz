<div class="container">
    <a href="/swatchdetail/{{ obj.id }}" class="card">
        <img class="card-img-top" src="{{ obj.image_back.url }}" alt="Card image cap">
        <div class="card-body">
            <h5 class="card-title">{{ obj.hex_color }}</h5>
            <p class="card-text">{{ obj.manufacturer }} {{ obj.color_name }}</p>
        </div>
        <div class="card-footer">
            <small class="text-muted">Uploaded {{ obj.date_added }}</small>
        </div>
    </a>
</div>
